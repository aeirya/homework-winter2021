                                                                         },
                                                                function(){
                                                                        var t = setTimeout(function() {
                                                                                 jQuery(".taglib-my-places").slideUp('slow');
                                                                            }, 400);
                                                                            jQuery(this).data('timeout', t);                                                 
                                                                }
                                                );
                  });
</script>
